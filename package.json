{
    "name": "acm-csu-website",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "start:client": "npm run start --workspace=client",
        "build:client": "npm run build --workspace=client",
        "start:server": "npm run dev --workspace=server",
        "dev": "concurrently \"npm run start:client\" \"npm run start:server\"",
        "start": "node server/server.js"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "dependencies": {
        "@emailjs/browser": "^4.4.1",
        "axios": "^1.9.0",
        "date-fns": "^4.1.0",
        "dotenv": "^16.5.0",
        "node-cache": "^5.1.2",
        "react-big-calendar": "^1.18.0"
    }
}
